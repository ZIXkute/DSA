//BFS
#include <stdio.h>
#include <stdlib.h>

typedef struct node {
    int data;
    struct node *left;
    struct node *right;
} Node;

Node* createNode(int data) {
    Node *newNode = (Node*)malloc(sizeof(Node));
    newNode->data = data;
    newNode->left = NULL;
    newNode->right = NULL;
    return newNode;
}

#define MAX 100

Node* queue[MAX];
int front = 0;
int rear = 0;

void enqueue(Node *node) {
    queue[rear++] = node;
}

Node* dequeue() {
    return queue[front++];
}

int isEmpty() {
    return front == rear;
}

void levelOrder(Node *root) {
    Node *current;

    if (root == NULL) return;

    front = rear = 0;
    enqueue(root);

    while (!isEmpty()) {
        current = dequeue();
        printf("%d ", current->data);

        if (current->left != NULL)
            enqueue(current->left);

        if (current->right != NULL)
            enqueue(current->right);
    }
}

int main(void) {
    Node *root;

    root = createNode(1);
    root->left = createNode(2);
    root->right = createNode(3);
    root->left->left = createNode(4);
    root->left->right = createNode(5);

    printf("BFS - Level Order: ");
    levelOrder(root);
    printf("\n");

    return 0;
}
